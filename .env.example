# ==================== .env.example ====================

"""
# Web Assistant Environment Variables

# Database
DATABASE_PATH=data/web_assistant.db

# LLM - LM Studio
LM_STUDIO_URL=http://localhost:1234/v1
LM_STUDIO_MODEL=local-model

# LLM - Ollama
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Application
DEBUG=False
LOG_LEVEL=INFO

# Selenium
SELENIUM_BROWSER=chrome
SELENIUM_HEADLESS=False

# Cache
CACHE_ENABLED=True
CACHE_TTL=86400
CACHE_MAX_SIZE=1000